{
    "manifest_version": 3,
    "name": "URL Saver",
    "version": "1.0",
    "description": "A Chrome extension to save and view URLs.",
    "permissions": [
        "activeTab",
        "tabs"
    ],
    "background": {
        "service_worker": "background.js"
    },
    "action": {
        "default_title": "URL Saver",
        "default_popup": "index.html"
    }
}